<UserControl x:Class="Eksamensprojekt_Final_1_WPF_API.Views.HomeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Eksamensprojekt_Final_1_WPF_API.Views"
             xmlns:vm="clr-namespace:Eksamensprojekt_Final_1_WPF_API.ViewModels"
             d:DataContext="{d:DesignInstance Type=vm:HomeViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel  Margin="200,50" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <Label Style="{StaticResource CustomLabelStyle}" Content="Chats" FontSize="26" HorizontalAlignment="Center"/>
            <ListBox Style="{StaticResource CustomListBoxStyle}" MinHeight="300" ItemsSource="{Binding Chats}"/>
        </StackPanel>
    </Grid>
</UserControl>
